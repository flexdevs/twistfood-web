<body>

  <div class="hero_area">
    <div class="bg-box">
      <img src="https://localhost:7049/Assets/images/hero-bg.jpg" alt="">
    </div>
    <!-- header section strats -->
    <header class="header_section">
      <div class="container">
        <nav class="navbar navbar-expand-lg custom_nav-container ">
          <a class="navbar-brand" href="index.html">
            <span>
              TwistFood
            </span>
          </a>

          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class=""> </span>
          </button>

          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav  mx-auto ">
              <li class="nav-item active">
                                <a class="nav-link" asp-controller="home" asp-action="index">Asosiy <span class="sr-only">(current)</span></a>
              </li>
              <li class="nav-item">
                                <a class="nav-link" asp-controller="products" asp-action="index">Menu</a>
              </li>
              <li class="nav-item">
                                <a class="nav-link" asp-controller="home" asp-action="about">Biz haqimizda</a>
              </li>
            </ul>
            <div class="user_option">
                            @await Component.InvokeAsync("DetailsCount")
                            @await Component.InvokeAsync("Identity")
            </div>
          </div>
        </nav>
      </div>
    </header>
    </div>
    <!-- end header section -->
        <div class="container" id="start">
            <div class="row">
                <div class="col-lg-12">
                    <div class="page-content">
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>

  <!-- footer section -->
  <footer class="footer_section">
    <div class="container">
        <div class="footer-col">
          <div class="footer_detail">
            <a href="" class="footer-logo">
              TwistFood
            </a>
            <p>
              Necessary, making this the first true generator on the Internet. It uses a dictionary of over 200 Latin words, combined with
            </p>
