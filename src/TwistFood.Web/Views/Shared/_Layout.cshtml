                        <a href="#"><i class="socicon-telegram text-primary mr-5"></i></a>
                <a href="#" class="btn btn-icon btn-light-instagram me-5 "><i class="fab fa-instagram fs-4"></i></a>
                <a href="#" class="btn btn-icon btn-light-youtube me-5 "><i class="fab fa-youtube fs-4"></i></a>
            </div>*@
          </div>
        </div>
      <div class="footer-info">
        <p>
                    &copy; <span id="displayYear"></span> <a class="link-dark" href="https://github.com/flexdevs">FlexDevs</a>
        </p>
      </div>
    </div>
  </footer>
  <!-- footer section -->

  <!-- jQery -->

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
